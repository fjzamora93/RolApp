  <p class="texto-accesibilidad">Accesibilidad:</p>
  <ul id="gmDropdown" class="dropdown-content">
    <li><button type="button" id="aumentar-letra">A <i class="fas fa-plus"></i></button></li>
    <li> <button type="button" id="disminuir-letra">A <i class="fas fa-minus"></i></button></li>
    <li><button type="button" id="mejorar-contraste"><i class="fas fa-adjust"></i></button></li>
    <li class="restablecer"> <button type="button" id="resetear">Restablecer</button></li>
   </ul>
</div>

<script>

    // Inicio - Abrir y cerrar dropdown + iconos 
    function mobileMenuOpen() {
        document.getElementById("gmDropdown").classList.toggle("show"); 
    };
    

    $("#tgr-wp-accesibilidad .dropbtn").click(function () {
        $('#tgr-wp-accesibilidad .dropbtn i.fas').toggleClass('fa-chevron-up fa-chevron-down');
    });
    // Fin - Abrir y cerrar dropdown + iconos
    

    
    $("#mejorar-contraste").click(function() {
        $(".pojo-a11y-btn-high-contrast").click();

    });

    $("#aumentar-letra").click(function() {
        $(".pojo-a11y-btn-resize-plus").click();
  
    });

    $("#disminuir-letra").click(function() {
        $(".pojo-a11y-btn-resize-minus").click();
       
    });
    
    $("#resetear").click(function() {
       $(".pojo-a11y-btn-reset").click();
    
    });
    // Fin - Accesibilidad

</script>		</div>
				</div>
					</div>
		</div>
							</div>
		</section>
