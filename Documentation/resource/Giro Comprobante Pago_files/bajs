<script>
$(document).ready(function(){
    $(window).scroll(function() {
        if ($(window).scrollTop() > 10 && document.documentElement.clientWidth <= 768) {
            $('.row-buscador').addClass('floatingNav');
        } else {
            $('.row-buscador').removeClass('floatingNav');
        }
    });
});
</script>
<!-- fin. AL SER STICKY, SE APLICA SOMBRA -->

<script>

//mejoras v10 24-11-2020:
//limpiar tildes 
//mejoras v9:
//Integracion de busqueda con centro de ayuda  
//Se comenta formualrio de envio de sugerencias
//Se modifican id
//Se crean variables para textos

function quitarTildes(cadena){
	const acentos = {'á':'a','é':'e','í':'i','ó':'o','ú':'u','Á':'A','É':'E','Í':'I','Ó':'O','Ú':'U'};
	return cadena.split('').map( letra => acentos[letra] || letra).join('').toString();	
}

$(document).ready(function(){
    var ul, li, a, aa, att, text, txtValue, menutitulo1="", span1, liMenu1;

    var textoSinResultados="No encontramos el trámite, intenta nuevamente. ";
    var textoIrCentroAyuda="Continúa la búsqueda en nuestro Centro de Ayuda";
    
    var textoFormLabel="Ayudanos a mejorar el buscador";
    var textoFormPlaceholder="Escribe aquí el tramite que buscabas…";
    var textoFormBoton="Enviar";
    var textoFormEnviado="!Gracias por ayudarnos a mejorar¡";

    ul = document.getElementById("ubermenu-nav-main-19");  
    a = ul.querySelectorAll("a");  
   // console.log(a.length);

    var ret = document.createElement('ul');
    ret.setAttribute("id", "tgr-lista-menu-tramites");
    ret.setAttribute("style", "display:none");
    
    var auxHref="";
    
  
  for (i = 0; i < a.length ; i++) { 
 
     if(a[i].href.indexOf("#")<0){ 
      // console.log(i+": "+a[i].href); 

         li = ret.appendChild(document.createElement('li'));
         aa = li.appendChild(document.createElement('a')); 

          att = document.createAttribute("href");        // Create a "href" attribute
          att.value = a[i].href;            // Set the value of the href attribute
          aa.setAttributeNode(att);                      // Add the href attribute to <a>
          att = document.createAttribute("href");        // Create a "href" attribute
         
         //prueba cambio de entorno
       //  console.log(a[i].href.replaceAll("entorno", "test.tesoreria.cl"));
          auxHref= a[i].href.replaceAll("entornowp", "www.tesoreria.cl");
       //   console.log(auxHref);
          att.value = auxHref;
          a[i].href = auxHref;
          //fin prueba
          //att.value = a[i].href;            // Set the value of the href attribute
          aa.setAttributeNode(att); 
          aa.setAttribute("data-title", a[i].title);                     // Add the href attribute to <a>
          aa.setAttribute("id", "tgr-menu-search-list-"+i);
          
// nuevo 15 julio 24 ----------------------------------------------------------------
           // Agregar target _blank si existe
            if (a[i].getAttribute("target") === "_blank") {
                aa.setAttribute("target", "_blank");
            }

          try{
           txtValue = a[i].textContent || a[i].innerText ;
             }catch(e){ 
          } 

          text = document.createTextNode(txtValue);
          aa.appendChild(text);
          
// nuevo 15 julio 24 ----------------------------------------------------------------
          // Agregar icono si el enlace tiene target _blank
            if (aa.getAttribute("target") === "_blank") {
                var iconSpan = document.createElement('span');
                iconSpan.className = "fas fa-external-link-alt";
                aa.appendChild(iconSpan);
            }
            
// nuevo 15 julio 24 ---------------------------------------------------------------- 
            // Añadir elemento al listado
            ret.appendChild(li);
            

          if(menutitulo1!=""){
              span1= li.appendChild(document.createElement('span'));
              $(span1).text(" - "+menutitulo1);        

          }


          a[i].removeAttribute("title");  //quitamos atributo a ubermenu
       // console.log(i+": "+a[i].title);
       }else{
          if( (a[i].parentElement.className).indexOf( "ubermenu-item-level-0" )>-1){
           // console.log("menu nivel-1");
            menutitulo1=a[i].textContent || a[i].innerText ;
          }
       } 

    }


//agregamos li sin resultados
    li = ret.appendChild(document.createElement('li'));
    li.setAttribute("id", "tgr-lista-menu-tramites-sin-res");
    span=li.appendChild(document.createElement("span"));
    span.setAttribute("id","tgr-lista-menu-tramites-sin-res-texto");
    span.appendChild(document.createTextNode(textoSinResultados));

    aa = li.appendChild(document.createElement('a'));
    aa.setAttribute("id", "tgr-menu-search-list-to-centro-ayuda"); 
    aa.setAttribute("href","#");
    aa.setAttribute("target","_blank");
    aa.setAttribute("style", "display:none");
    aa.appendChild(document.createTextNode(textoIrCentroAyuda));

   

//agregamos un formulario de sugenrencias
    var div= li.appendChild(document.createElement("div"));
    div.setAttribute("id", "tgr-lista-menu-tramites-sin-res-enviar");
    div.setAttribute("style", "display:none");

    var label =div.appendChild(document.createElement("label"))   
    label.appendChild(document.createTextNode(textoFormLabel));

    var input = div.appendChild(document.createElement("input"))
    input.setAttribute("type", "text");
    input.setAttribute("id", "tgr-input-comentario-buscador");
    input.setAttribute("placeholder",textoFormPlaceholder );
    input.setAttribute("maxLength","75");

    var button = div.appendChild(document.createElement("button"))
    button.setAttribute("type", "button");
    button.setAttribute("id", "tgr-boton-comentario-buscador");   
    button.innerHTML=textoFormBoton;
    button.addEventListener("click", tgrBuscadorEnviarComentario);

    var div2= li.appendChild(document.createElement("div"));
    div2.setAttribute("id", "tgr-lista-menu-tramites-sin-res-enviar-gracias");
    div2.setAttribute("style", "display:none");

    var span2 =div2.appendChild(document.createElement("span"))   
    span2.appendChild(document.createTextNode(textoFormEnviado));
//agregamos un formulario de sugenrencias




  $("#tgr-lista-menu-buscador").append(ret);

  document.getElementById("tgr-input-buscador-tramites").addEventListener("search", function(event) {
    $("#tgr-lista-menu-tramites").hide(); 
    $("#tgr-lista-menu-buscador").removeClass( "open-search");

    });
  
});




function tgrBuscadorEnviarComentario() {
  var comentario=$("#tgr-input-comentario-buscador").val();
  var textoBuscado=$("#tgr-input-buscador-tramites").val();

//dejar comentado
/* gtag('event', 'Sugerencias de búsqueda', {
  'event_category' : 'Buscador de Trámites',
  'event_label' : textoBuscado+" - " +comentario
});*/

$("#tgr-lista-menu-tramites-sin-res-enviar").hide();
$("#tgr-lista-menu-tramites-sin-res-enviar-gracias").show();

 console.log("Enviamos la informacion: "+ textoBuscado+" - " +comentario)
  var comentario=$("#tgr-input-comentario-buscador").val("");
  var textoBuscado=$("#tgr-input-buscador-tramites").val("");
}



//descomentar en produccion evento para tag manager
// dataLayer = [];

// function tgrEnviarEventoBusqueda(etiqueta){
    
//     if(etiqueta==""){
//         etiqueta="En blanco";
//     }
    
//     dataLayer.push({     'categoria': 'Buscador de Trámites',     'accion': 'Palabras claves de búsqueda',     'etiqueta': etiqueta });
    
