  <script>
  
  function compararDominioReferrer() {
    try {
        
        const dominiosPermitidos = ["web.tesoreria.cl", "tgr.cl", "autentica.tgr.cl", "tesoreria.cl"];
        
        const referrer = document.referrer;
        const urlReferrer = new URL(referrer);
        const dominioReferrer = urlReferrer.hostname;
        console.log("dominioReferrer: "+dominioReferrer);

        // Comparar el dominio del referrer con los dominios permitidos
        if (dominiosPermitidos.includes(dominioReferrer)) {
          console.log("Referrer permitido.");
        } else {
          console.log("Referrer No permitido.");
                return false;
        }
      } catch (error) {
        console.log("No se puede obtener el referrer o el formato no es válido.");
          return false;
      }
      
      return true;
    }
    
    function asignarNombres(url){
     
       var nombreCompleto = localStorage.getItem("tgr-usuario-nombres"); 
       var pagePath = window.location.pathname;
          
          
       if(nombreCompleto != null && nombreCompleto != "" && nombreCompleto != false && nombreCompleto != undefined){
                 
                 $("#tgr-micuenta-nombres").text(nombreCompleto);
             
                 // Comparar si el PagePath es igual a "/inicio/"
                if (pagePath === "/inicio/") {
                     var span = document.getElementById("tgr-usuario-nombres");
                     var textoBienvenida="Hola, "+nombreCompleto;
                     //var  txt = document.createTextNode(textoBienvenida);
                      span.textContent=textoBienvenida;
                }
       }else{
           
           

           
            var paramNo = url.searchParams.get('no');
            var paramAp = url.searchParams.get('ap');
            
            const currentDomain = window.location.hostname;
             if(currentDomain==="web.tesoreria.cl"){
           
                nombreCompleto=paramNo+" "+paramAp;
                nombreCompleto=nombreCompleto.trim();
                
                $("#tgr-micuenta-nombres").text(nombreCompleto);
                localStorage.setItem("tgr-usuario-nombres", nombreCompleto);
                
               var pagePath = window.location.pathname;

                // Comparar si el PagePath es igual a "/inicio/"
                if (pagePath === "/inicio/") {
                     var span = document.getElementById("tgr-usuario-nombres");
                     var textoBienvenida="Hola, "+nombreCompleto;
                     span.textContent=textoBienvenida;
                }
             }
       }
     
 }
  
  function mostrarSesionIniciadaTGR(){
     
        var url_string = window.location.href;
        var url = new URL(url_string);
        var paramGo = url.searchParams.get('go');

        console.log("referrer inicio: "+document.referrer);
     
        //se carga con parametro retonado desde la autenticacion
        if(paramGo != null && compararDominioReferrer()) {
           
            asignarNombres(url);
        } else {
             asignarNombres(url);
        }

      $("#login-wp--iniciar-sesion").hide();
      $("#login-wp--sesion-iniciada").show();
      $("#login-wp--contenedor").toggleClass("invisible");
      $("#login-wp--cerrar-sesion--desktop").toggleClass("display-none");
      $(".tgr-logo-desktop a").attr("href", "/inicio/");
      $(".tgr-logo-mobile a").attr("href", "/inicio/");

}

function ocultarSesionIniciadaTGR(){

     console.log("sesion expirada wp");
     $("#login-wp--iniciar-sesion").show();
     $("#login-wp--sesion-iniciada").hide();
     $("#login-wp--contenedor").toggleClass("invisible");

}

function cerrarSesionTGR(){


    // window.location.href = "https://autentica.tgr.cl/logout?client_id=648m4flmisrgjags2vqvb41f49&logout_uri=https://www-qa-new.tegere.info/";
    localStorage.removeItem("tgr-usuario-nombres");
    window.location.href = "https://www.tesoreria.cl/LoginPortalCognito/logout";

} 


$( "#login-wp--iniciar-sesion" ).click(function() {
//  window.location.href = "https://www.tesoreria.cl/LoginPortalCognito/login?go=aumc2_w12&sp=%2Ftramites-tgr%2Fmi-cuenta-tgr%2F";

 window.location.href = "https://www.tesoreria.cl/LoginPortalCognito/login?go=aumc2_w12&sp=%2Finicio%2F";
 
//  window.location.href = "https://autentica.tgr.cl/oauth2/authorize?response_type=token&client_id=3d1mkfdi3fasmsqr2sep7g3ea8&redirect_uri=https://web.tesoreria.cl/";
 
});


$( "#login-wp--cerrar-sesion--mobile, #login-wp--cerrar-sesion--desktop" ).click(function() {
 cerrarSesionTGR();
});

    
// Here "addEventListener" is for standards-compliant web browsers and "attachEvent" is for IE Browsers.
var eventMethod = window.addEventListener ? "addEventListener" : "attachEvent";
var eventer = window[eventMethod];

// Now...
// if 
//    "attachEvent", then we need to select "onmessage" as the event. 
// if 
//    "addEventListener", then we need to select "message" as the event

var messageEvent = eventMethod == "attachEvent" ? "onmessage" : "message";

// Listen to message from child IFrame window
eventer(messageEvent, function (e) {
    //   alert(e.data);
       // Do whatever you want to do with the data got from IFrame in Parent form.
       console.log("e.data Object: "+Object.values(e.data));
       /*var texto=e.data.toString();
       console.log("Mensajes desde Iframe:" +texto);*/
       if(e.data=="SI"){
            console.log("inicio-wp:  SI");
            mostrarSesionIniciadaTGR();
       }else if(e.data=="NO"){
            console.log("inicio-wp:  NO");
            ocultarSesionIniciadaTGR();
       }
}, false);
      
  </script>		</div>
				</div>
					</div>
		</div>
							</div>
		</section>
					</div>
		</div>
							</div>
		</section>
